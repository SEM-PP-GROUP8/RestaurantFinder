    <bean id="controller" class = "com.group8.controller.Controller">
        <constructor-arg>
            <bean id="editRestWindow" class = "com.group8.view.EditRestaurant">

            </bean>
        </constructor-arg>
        <constructor-arg>
            <bean id="loginWindow" class = "com.group8.view.Login">

            </bean>
        </constructor-arg>
        <constructor-arg>
            <bean id="queryWindow" class = "com.group8.view.Query">

            </bean>
        </constructor-arg>
        
        <constructor-arg>
            <bean id="registerWindow" class = "com.group8.view.Register">

            </bean>
        </constructor-arg>
        
        <!--/*KIKE _ USER*/-->
        <constructor-arg>
            <bean id="userView" class = "com.group8.view.User">

            </bean>
        </constructor-arg>
       
        <!--/*KIKE _ OWNER*/-->
